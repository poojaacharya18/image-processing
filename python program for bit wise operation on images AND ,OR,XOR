XOR:
from PIL import Image, ImageChops 
     
# creating a image1 object 
im1 = Image.open(r"C:/Users/User/Pictures/Saved Pictures/pic/Sunflower_from_Silesia2.jpg") .convert("1")
     
# creating a image2 object 
im2 = Image.open(r"C:/Users/User/Pictures/Saved Pictures/pic/photo-1562690868-60bbe7293e94.jpg") .convert("1")
     
# applying logical_xor method 
im3 = ImageChops.logical_xor(im1, im2) 
     
im3.show() 

OR:
from PIL import Image, ImageChops 
    
# creating a image1 object 
im1 = Image.open(r"C:/Users/User/Pictures/Saved Pictures/pic/Sunflower_from_Silesia2.jpg") .convert("1")
    
# creating a image2 object 
im2 = Image.open(r"C:/Users/User/Pictures/Saved Pictures/pic/photo-1562690868-60bbe7293e94.jpg") .convert("1")
    
# applying logical_or method 
im3 = ImageChops.logical_or(im1, im2) 
    
im3.show() 

AND:
from PIL import Image, ImageChops 
    
# creating a image1 object 
im1 = Image.open(r"C:/Users/User/Pictures/Saved Pictures/pic/Sunflower_from_Silesia2.jpg") .convert("1")
    
# creating a image2 object 
im2 = Image.open(r"C:/Users/User/Pictures/Saved Pictures/pic/photo-1562690868-60bbe7293e94.jpg") .convert("1")
    
# applying logical_and method 
im3 = ImageChops.logical_and(im1, im2) 
    
im3.show()
